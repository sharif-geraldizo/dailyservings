}

/* begin css tabs */

ul.tabnav { /* general settings */
    text-align: left; /* set to left, right or center */
    margin: 0; /* set margins as desired */
    /* border-bottom: 1px solid #333; set border COLOR as desired */
    font-weight: bold;
    list-style-type: none;
    float: left;
    width: auto;
    padding: 3px 0 0 0; /* THIRD number must change with respect to padding-top (X) below */
}
ul.tabnav li { /* do not change */
    float: left;
}
body#tab1 li.tab1, body#tab2 li.tab2, body#tab3 li.tab3, body#tab4 li.tab4 { /* settings for selected tab */
    /* set background color to match above border color */
}
body#tab1 li.tab1 a, body#tab2 li.tab2 a, body#tab3 li.tab3 a, body#tab4 li.tab4 a { /* settings for selected tab link */
    background-color: #dcdcdc; /* set selected tab background color as desired */
    color: #000; /* set selected tab link color as desired */
    position: relative;
    /*top: 1px;*/
    float: left;
    padding: 5px 10px; /* must change with respect to padding (X) above and below */
}
ul.tabnav li a { /* settings for all tab links */
    padding: 5px 10px; /* set padding (tab size) as desired; FIRST number must change with respect to padding-top (X) above */
    /*border: 1px solid #333; set border COLOR as desired; usually matches border color specified in #tabnav */
    background-color: #333; /* set unselected tab background color as desired */
    color: #fff; /* set unselected tab link color as desired */
    margin-right: 1px; /* set additional spacing between tabs as desired */
    text-decoration: none;
    border-bottom: none;
    /*-moz-border-radius: 3px 3px 0 0;
    -webkit-border-radius: 3px 3px 0 0;*/
    float: left;
    font-size: 0.8em !important;
    text-transform: uppercase;
}
ul.tabnav a:hover { /* settings for hover effect */
    background: #000; /* set desired hover color */
    color: #fff;
}
ul.tabnav li a img {
    vertical-align: middle;
}
.tab-box, .tab-box-wide {
    /* border: 1px solid #333; */
    border-top: 0;
    margin: 0 0 20px 0;
    background-color: #dcdcdc;
    float: left;
}
.tab-box {
    padding: 10px 10px 0 10px;
}
.tab-box-wide {
    width: 657px;
    padding: 10px;
}
/* end css tabs */

/* slideshow */
#cat-slides {
    margin: 0;
    padding: 0;
    position: relative;
    width: 100%;
    height: 300px;
    background: #090909 url(../images/bg-pattern3.png) top left repeat;
    border-bottom: 4px solid #000;
}
.cat-slides-indent {
    position: relative;
    height: 300px;
}
/* container for slides */
.images {
    background:#000;
    position:relative;
    height:300px;
    width:960px;
    float:left;
    cursor:pointer;
}
/* single slide */
.images div {
    display:none;
    position:absolute;
    top:0;
    left:0;
    height:300px;
    width: 960px;
    color: #000;
}
.images div a {
    float: left;
}
.slider-img {
    float: left;
    position: relative;
    z-index: 1;
}
.slider-text {
    width: 500px;
    height: 280px;
    /*background: url(../images/slider-text-bg.png) top left repeat;*/
    color: #fff;
    padding: 40px 10px 10px 10px;
    position: absolute;
    top: 0;
    right: 0;
    z-index: 2;
}
.slider-text span.slider-head {
    font-size: 1.571em;
    font-weight: 800;
    display: block;
    color: #fff;
    text-transform: uppercase;
    margin: 0 0 10px 0;
}
.slider-text-btn {
    display: block;
    width: 96px;
    height: 33px;
    padding: 5px 0 0 0 !important;
    margin: 15px 0 0 0;
    text-align: center;
    background: url(../images/slider-text-btn.png) 0 0 no-repeat;
    color: #fff !important;
    text-decoration: none !important;
}
.slider-text-btn:hover {
    color: #ffcc00 !important;
}
/* play and stop buttons */
.slider-button {
    float: right;
    font-size: 0.8em;
    margin: 7px 10px 0 0;
}
.slider-button a {
    text-decoration: none;
    color: #333;
}
.slider-button a:hover {
    color: #ba1020;
}
/* prev and next buttons */
.forward, .backward {
    float:left;
    margin-top:140px;
    /*background:#fff url() no-repeat; -- put your arrow image here */
    display:block;
    width:30px;
    height:30px;
    cursor:pointer;
    font-size:1px;
    text-indent:-9999em;
}
/* next */
.forward {
    background-position: 0 -30px;
    clear:right; }
.forward:hover { background-position:-30px -30px; }
.forward:active	{ background-position:-60px -30px; }
/* prev */
.backward:hover { background-position:-30px 0; }
.backward:active { background-position:-60px 0; }
/* disabled navigational button. is not needed when tabs are configured with rotate: true */
.disabled {
    visibility:hidden !important;
}
